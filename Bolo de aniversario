# -*- coding: utf-8 -*-
def quant(arg): #bolo de aniversário
  idade = arg
  sopro = input()
  forte = 'fuuuuuuu'
  medio = 'fuuu'
  fraco = 'fu'

  while arg != 0:
    if sopro == forte:
      arg = arg - 1
      print("bom sopro!")
      if arg > 0:
       sopro = input()
    
    elif sopro == medio:
      print("um pouco mais de força no sopro!")
      sopro = input()
    
    elif sopro == fraco:
      print("precisa de muito mais força no sopro!")
      sopro = input()

  
  print("Parabéns para pelo seu aniversário de", idade ,"anos!" )


quant(int(input()))
